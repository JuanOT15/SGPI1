@model SGPI.Models.Homologacion
@{
    ViewData["Title"] = "Buscar";
}





<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <style>
        .header {
            text-align: center;
            height: 800px;
            padding: 12px;
            background-image: url("https://img.freepik.com/vector-gratis/fondo-onda-blanca-suave_52683-55288.jpg?t=st=1653232988~exp=1653233588~hmac=89bc53c15fef27946a9950001b052139b34bc5c175693c7c6cfdb6789e795fb5&w=1380")
        }
    </style>
</head>
<body>
    <style>
        body {
            background-image: url('https://static.vecteezy.com/system/resources/previews/001/176/927/original/blue-and-white-abstract-background-vector.jpg');
            background-repeat: no-repeat;
            background-attachment: fixed;
            background-size: 100% 100%;
        }
    </style>
    <div class="header">
        <br>
        <h1><center>Buscar Homologacion</h1></center>
        <br />
        <br />
        <br />
        <br />
        <br />
        <br />
        <br />
        <br />
        <br />
        <form method="post">
            <div class="barra-busqueda">
                <label asp-for="Universidad">Universidad</label>
                <input asp-for="Universidad" type="text" placeholder="Universidad">
                <button type="submit" class="btn btn-secondary">Buscar</button>
            </div>

            <br />
            <br />
            <div class="bltbl">
                <table style="width: 100%">
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.FechaHomologacion)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Universidad)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Nota)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.IdPrograma)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.IdModulo)
                        </th>
                        
                        



                    </tr>
                    <tr>
                        <td class="td nombre-usuario">@Html.DisplayFor(modelItem => modelItem.FechaHomologacion)</td>
                        <td class="td apellido-usuario">@Html.DisplayFor(modelItem => modelItem.Universidad)</td>
                        <td class="td documento-usuario">@Html.DisplayFor(modelItem => modelItem.Nota)</td>
                        <td class="td rol-usuario">@Html.DisplayFor(modelItem => modelItem.IdPrograma)</td>
                        <td class="td genero-usuario">@Html.DisplayFor(modelItem => modelItem.IdModulo)</td>
                    </tr>
                </table>
                <br />
                <br /><br /><br /><br /> <br /> <br /> <br /> <br /> <br /> <br />
                <br />
                <a href="https://localhost:44384/Coordinador/MenuCoordinador" <button type="button" class="btn  btn-outline-secondary float-right">Volver</button></a>
            </div>
        </form>
    </div>

</body>

</html>